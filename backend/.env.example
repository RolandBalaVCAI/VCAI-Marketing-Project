# Backend Environment Variables

# Peach AI API Configuration
PEACHAI_API_TOKEN=your_peach_ai_token_here
PEACHAI_API_URL=https://api.peach.ai
PEACHAI_API_TIMEOUT=30

# Database Configuration
DATABASE_URL=sqlite:///./datawarehouse.db
DATABASE_BACKUP_PATH=./backups/
DATABASE_MAX_CONNECTIONS=10

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
API_CORS_ORIGINS=http://localhost:3000,http://localhost:5173

# Google Sheets Configuration (optional)
GOOGLE_CREDENTIALS_FILE=./config/google-credentials.json
GOOGLE_SHEET_ID=your_google_sheet_id_here

# ETL Configuration
ETL_BATCH_SIZE=200
ETL_MAX_WORKERS=5
ETL_QUALITY_THRESHOLD=0.8

# Logging
LOG_LEVEL=INFO
LOG_FILE=./logs/api.log

# Development
DEBUG=true
RELOAD=true